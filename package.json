{
  "name": "testservice",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js && node dist/migrations/migrate.js",
    "dev": "nodemon src/index.ts",
    "dev2": "nodemon src/grpc/server.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prettier": "prettier --write src",
    "prepare": "husky || true",
    "test:migrate": "npx dotenv -e .env.test -- npx prisma migrate reset -f",
    "test": "npm run test:migrate && jest --runInBand --verbose --coverage",
    "build-docker-image": "docker build -t testservice .",
    "start:prod": "npx prisma db push && node src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "@grpc/proto-loader": "^0.7.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-levenshtein": "^3.0.0",
    "kafkajs": "^2.2.4",
    "kysely": "^0.27.4",
    "mtogo-proto-provider": "^1.0.9",
    "node-fetch": "^3.3.2",
    "pg": "^8.13.0",
    "sanitize-html": "^2.14.0",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@faker-js/faker": "^9.2.0",
    "@prisma/client": "^6.1.0",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/jest": "^29.5.14",
    "@types/pg": "^8.11.10",
    "@types/sanitize-html": "^2.13.0",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "artillery": "^2.0.21",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.12.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
